title = {{The INDEX Project: Critical appraisal of the setting and implementation of indoor exposure limits in the EU}},
url = {https://ec.europa.eu/health/ph{\_}projects/2002/pollution/fp{\_}pollution{\_}2002{\_}frep{\_}02.pdf},
year = {2004}
