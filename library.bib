@phdthesis{Kelway2002,
abstract = {The burden of automobile dependence is one of London's most intractable problems, yet the capital's urban fabric remains inherently car orientated. Planning and Urban Design mechanisms have a seemingly decisive role in the quest to alleviate the supremacy of cars, however their contribution is frequently undervalued and under researched, particularly in the context of rational objectives within tangible localities. This thesis therefore adopts a cross case analysis involving six naturalistic case study neighbourhoods and seeks to elucidate the influence of the pedestrian environment upon the perception and behaviour of users and hence the potential role of pedestrian orientated planning and design in reducing the need to travel by car. The investigation also endeavours to determine whether public demand for enhanced pedestrian provision exists within London and embraces issues relating to the composition and configuration of successful pedestrian orientated neighbourhoods. The thesis provides compelling evidence that the quality of the pedestrian environment influences use levels, intrinsic behaviour and travel preferences during periods when pedestrians are engaged in unconstrained activity. Consequently, it is concluded that superior pedestrian provision can appreciably contribute to the alleviation of automobile dependence within London. However, there is no evidence of universal public consensus supporting an enhanced pedestrian realm throughout London, although the interview survey revealed fervent calls for improvements at the neighbourhood scale. The investigation also suggests that qualities of compactness and heterogeneity are integral attributes of a successful pedestrian orientated neighbourhood as are certain designable facets; notably a provision for personal wellbeing, unimpeded movement and a unique identity. It is concluded that the concentric neighbourhood model can reconcile these attributes, though should not be slavishly adhered to and must enable flexible, neighbourhood specific adaptations. However, it is proposed that a more progressive political standpoint is also required before pedestrian orientated neighbourhoods can be delivered throughout London.},
author = {Kelway, Benjamin John Bevan},
file = {:C$\backslash$:/Users/sammy/Desktop/Work/UCL/GEOG0114 Principles of Spatial Analysis/Coursework 1/Reading/The pro-pedestrian restructuring of urban space - A critical analysis of land-use planning and urban design solutions to the intensifying burden of automobile dependance within London.pdf:pdf},
keywords = {Transportation planning,Urban planning},
pages = {212},
school = {University College London},
title = {{The pro-pedestrian restructuring of urban space: A critical analysis of land-use planning and urban design solutions to the intensifying burden of automobile dependance within London}},
type = {Dissertation/Thesis},
url = {https://search-proquest-com.libproxy.ucl.ac.uk/docview/1759337636?accountid=14511},
year = {2002}
}
